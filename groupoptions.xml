<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTDs/groupoptions.dtd">
<data>
	<import>
		<categories>
			<category name="admin.smallbutton">
				<parent>admin</parent>
			</category>
			<category name="user.smallbutton">
				<parent>user.board</parent>
			</category>
		</categories>
		<options>
			<option name="user.smallbutton.canUse">
				<categoryname>user.smallbutton</categoryname>
