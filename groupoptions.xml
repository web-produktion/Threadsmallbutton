			<option name="admin.smallbutton.canUse">
				<categoryname>admin.smallbutton</categoryname>
				<optiontype>boolean</optiontype>
				<showorder>1</showorder>
			</option>
			<option name="admin.smallbutton.canAdd">
				<categoryname>admin.smallbutton</categoryname>
				<optiontype>boolean</optiontype>
				<showorder>2</showorder>
			</option>
			<option name="admin.smallbutton.canEdit">
				<categoryname>admin.smallbutton</categoryname>
				<optiontype>boolean</optiontype>
				<showorder>3</showorder>
			</option>
			<option name="admin.smallbutton.canDelete">
				<categoryname>admin.smallbutton</categoryname>
				<optiontype>boolean</optiontype>
				<showorder>4</showorder>
			</option>
		</options>
	</import>
</data>